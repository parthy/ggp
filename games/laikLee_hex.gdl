    (true (control red)))

(<= (next (control red))
    (true (control blue)))

(<= (next (cell ?x ?y ?player))
    (does ?player (mark ?x ?y))
    (true (cell ?x ?y blank)))

(<= (next (cell ?x ?y ?color))
    (true (cell ?x ?y ?color))
    (distinct ?color blank))

(<= (next (cell ?x ?y blank))
    (does ?player (mark ?j ?k))
    (true (cell ?x ?y blank))
    (or (distinct ?x ?j) (distinct ?y ?k)))

